{
    "Experiment": "2DMisesFromAngles",
    "Exp_group":  "LossFuncTest2", 

    "ModelName":  "GraphSage_network_angles",

    "epochs":     100,
    "train_data": 2500000,
    "batch_size": 64,
    "early_stop": true,
    "patience":   3,
    "learning_rate":   3e-3,
    "lr_schedule": "classic_schedule",
    "val_every":   100000,
    "buffer_size": 50000,
    "verbose":     true,

    "hyper_params": {
        "n_out":           2,
        "n_kappa":         2,
        "n_corr":          0,
        "cossin":          false,
        "n_in":            6,
        "hidden_states":   64,
        "dropout":         0.25,
        "batch_norm":      true
    },

    "Data":            "MuonGunSRTAngles",
    "GraphType":       "NearestNeighbors",
    "GraphParam":       14,
    "data_split":      [0.95, 0.025, 0.025],
    "clear_dataset":   false,
    "graph_batch":     10000,
    "transforms":      "/home/johannbs/Desktop/IceCube_GNN/data/db_files/transformers.pkl",

    "raw_path":   "/home/johannbs/Desktop/IceCube_GNN/data/db_files/rasmus_classification_muon_3neutrino_3mio.db",

    "features":   ["dom_x", "dom_y", "dom_z", "time", "charge_log10", "SRTInIcePulses"],
    "targets":    ["azimuth", "zenith"],
    "event_lims": "pid = 13",
    "node_lims":  null, 

    "LossFunc":   "VonMisesPolarZenith_from_angles",
    "log_wandb": true,
    "metrics":   ["azimuthal_u_from_angles", "zenith_u_from_angles"]
}
   